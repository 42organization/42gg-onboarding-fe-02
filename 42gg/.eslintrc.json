{
  "extends": ["prettier", "next/core-web-vitals", "airbnb", "airbnb/hooks"],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ],
    "react/react-in-jsx-scope": "off",
    "linebreak-style": "off"
  }
}
